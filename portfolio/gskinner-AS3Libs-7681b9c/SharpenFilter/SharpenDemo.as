package  {		import com.gskinner.filters.SharpenFilter;	import flash.display.Sprite;	import flash.display.MovieClip;		public class SharpenDemo extends Sprite {			// Public Properties:		public var pic1:MovieClip;		public var pic2:MovieClip;		public var pic3:MovieClip;			// Initialization:		public function SharpenDemo() {			var sf1:SharpenFilter = new SharpenFilter(0.2);			pic1.filters = [sf1];						var sf2:SharpenFilter = new SharpenFilter(0.5);			pic2.filters = [sf2];						var sf3:SharpenFilter = new SharpenFilter(1);			pic3.filters = [sf3];		}			}	}