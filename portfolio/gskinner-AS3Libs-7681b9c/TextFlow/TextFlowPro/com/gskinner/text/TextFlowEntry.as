package com.gskinner.text {		import flash.text.TextField;		public class TextFlowEntry {				public var textField:TextField;		public var alwaysShowSelection:Boolean;		public var selectable:Boolean;		public var type:String;		public var trim:int=0;		public var endIndex:int=0;		public var beginIndex:int=0;				public function TextFlowEntry(textField:TextField) {			this.textField = textField;			alwaysShowSelection = textField.alwaysShowSelection;			selectable = textField.selectable;			type = textField.type;		}			}	}