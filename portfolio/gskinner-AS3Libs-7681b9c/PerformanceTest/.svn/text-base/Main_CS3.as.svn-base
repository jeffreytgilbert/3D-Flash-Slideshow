package  {		import flash.display.MovieClip;	import flash.text.TextField;	import flash.events.Event;	import performancetests.numeric.*;	import performancetests.*;	import com.gskinner.utils.PerformanceTest;		public class Main_CS3 extends MovieClip {			// Constants:			// Public Properties:			// Protected Properties:			// Initialization:		public function Main_CS3() {			// this is mostly the same as the CS4 example, but removes all the tests that use Vector.			var perfTest:PerformanceTest = PerformanceTest.getInstance();			perfTest.out = out;			perfTest.testSuite(new Functions());			perfTest.testSuite(new Scope());			perfTest.testSuite(new ArrayIterators());			perfTest.testFunction(test, 5, "testFunction", "this is a quick test of testFunction");			perfTest.testRender(this, null, 10, "testRender", "this is a quick test of testRender");		}			// Public getter / setters:			// Public Methods:			// Protected Methods:		protected function out(str:*):void {			outFld.appendText(String(str)+"\n");			outFld.dispatchEvent(new Event(Event.CHANGE));			trace(str);		}				protected function test():void {			for (var i:int=0; i<100000; i++) {				Math.random();			}		}			}	}