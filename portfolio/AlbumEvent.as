package  {	import flash.events.Event;		public class AlbumEvent extends flash.events.Event {				public static const CHANGE_FULLSCREEN_PHOTO:String = 'changeFullscreenPhoto';		public static const LAUNCH_SLIDESHOW:String = 'launchSlideshow';				private var _metaData:XML;		public function get metaData():XML{ return _metaData; }		public function AlbumEvent(type:String, metaData:XML, bubbles:Boolean = false, cancelable:Boolean = false) {			// constructor code			super(type, bubbles, cancelable);			_metaData = metaData;		}		        public override function clone():Event        {            return new AlbumEvent(type, _metaData, bubbles, cancelable);        }               public override function toString():String        {            return formatToString("AlbumEvent", "type", "metaData", "bubbles", "cancelable");        }	}}